JobBatchName = "Saving_Xsum"
executable    = $ENV(HOME)/.conda/envs/lyc/bin/python
arguments     = $ENV(HOME)/graph2text/save_xsum_ds.py

universe         = vanilla

log    = c$(cluster).p$(process).log
output = c$(cluster).p$(process).out
error  = c$(cluster).p$(process).error

should_transfer_files = YES
environment = "mount=$ENV(HOME)"

request_CPUs     = 1
request_memory   = 4G

queue 1

